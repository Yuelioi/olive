{
    "compilerOptions": {
        "target": "ES2022",
        "esModuleInterop": true,
        "types": [
            "index.d.ts",
            "pinia",
        ],
        "baseUrl": "./",
        "paths": {
            "@/*": [
                "./src/*"
            ],
            "@/types/*": [
                "src/types/*"
            ],
            "@/assets/*": [
                "src/assets/*"
            ],
            "@/components/*": [
                "src/components/*"
            ],
            "@/configs/*": [
                "src/configs/*"
            ],
            "@/stores/*": [
                "src/stores/*"
            ]
        }
    },
    "files": [],
    "references": [
        {
            "path": "./tsconfig.node.json"
        },
        {
            "path": "./tsconfig.app.json"
        },
        {
            "path": "./tsconfig.vitest.json"
        }
    ]
}